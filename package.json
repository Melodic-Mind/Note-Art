        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/register": "^7.4.0",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.5",
        "babel-loader": "^8.0.4",
        "babel-plugin-istanbul": "^5.1.0",
        "chai": "^4.2.0",
        "cross-env": "^5.2.0",
        "dumper.js": "^1.3.1",
        "ink-docstrap": "^1.3.2",
        "jsdom": "^14.0.0",
        "jsdom-global": "^3.0.2",
