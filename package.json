{
    "name": "note-art",
    "version": "2.2.4",
    "description": "Music programming package",
    "homepage": "https://github.com/Seanitzel/Note-Art/tree/master",
    "repository": {
        "type": "git",
        "url": "https://github.com/Seanitzel/Note-Art.git"
    },
    "main": "dist/note-art.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "files": [
        "README.md",
        "LICENSE.md",
        "src",
        "docs",
        "dist"
    ],
    "scripts": {
        "dev": "webpack --mode development",
        "build": "rm -fr dist/* && tsc -p tsconfig.json && webpack --mode production",
        "prepare": "",
        "webpack:build": "webpack -p --env.production",
        "ts:build": "tsc",
        "ts:watch": "tsc --watch",
        "transpile": "npx babel src --out-dir lib",
        "test": "cross-env mocha -r ts-node/register -r esm  \"src/test/**/*.spec.js\"",
        "coverage": "cross-env NODE_ENV=test nyc npm run test -- --reporter landing --cache",
        "report": "nyc report --root coverage --format html",
        "docs": "vuepress-jsdoc -d docs -e src/validation index.d.ts",
        "docs:dev": "vuepress dev docs/",
        "docs:build": "vuepress build docs",
        "prepublish": "tsc",
        "predeploy": "npm version patch",
        "deploy": "npm publish"
    },
    "keywords": [
        "music",
        "art",
        "note",
        "chord",
        "key",
        "scale",
        "rhythm"
    ],
    "author": "Sean Dvir Seanitzel <seandvir12@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.13.10",
        "@babel/plugin-proposal-throw-expressions": "^7.12.13",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/register": "^7.13.8",
        "@types/chai": "^4.2.15",
        "@types/mocha": "^8.2.2",
        "amd-loader": "0.0.8",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "chai": "^4.3.4",
        "core-js": "^3.9.1",
        "cross-env": "^7.0.3",
        "dumper.js": "^1.3.1",
        "eslint": "^7.22.0",
        "esm": "^3.2.25",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0",
        "sinon": "^9.2.4",
        "sinon-chai": "^3.5.0",
        "source-map-loader": "^1.1.3",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3",
        "vuepress": "^1.8.2",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
    },
    "dependencies": {
        "tone": "^14.7.77"
    }
}
