        "dumper.js": "^1.3.1",
        "eslint": "^7.22.0",
        "mocha": "^8.3.2",
        "nyc": "^15.1.0",
        "sinon": "^9.2.4",
        "sinon-chai": "^3.5.0",
        "vuepress": "^1.8.2",
        "webpack": "^4.46.0",
        "webpack-cli": "^3.3.12"
