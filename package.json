{
    "name": "note-art",
    "version": "0.1.24",
    "description": "Music programming package",
    "main": "./dist/note-art.js",
    "homepage": "https://github.com/Seanitzel/Note-Art/tree/master",
    "repository": {
        "type": "git",
        "url": "https://github.com/Seanitzel/Note-Art.git"
    },
    "scripts": {
        "dev": "webpack --mode development",
        "build": "webpack --mode production",
        "prepare": "",
        "transpile": "npx babel src --out-dir lib",
        "test": "cross-env NODE_PATH=node_modules/tone/ mocha \"test/**/*.spec.js\"",
        "coverage": "cross-env NODE_ENV=test nyc npm run test -- --reporter landing --cache",
        "report": "nyc report --root coverage --format html",
        "docs": "vuepress-jsdoc -d docs -e src/validation index.d.ts",
        "docs:dev": "vuepress dev docs/",
        "docs:build": "vuepress build docs",
        "predeploy": "npm version patch",
        "deploy": "npm publish"
    },
    "keywords": [
        "music",
        "art",
        "note",
        "chord",
        "key",
        "scale",
        "rhythm"
    ],
    "author": "Sean Dvir Seanitzel <seandvir12@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-proposal-throw-expressions": "^7.2.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/register": "^7.4.0",
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "amd-loader": "0.0.8",
        "babel-loader": "^8.0.6",
        "babel-plugin-istanbul": "^5.2.0",
        "chai": "^4.2.0",
        "core-js": "^2.6.10",
        "cross-env": "^5.2.1",
        "dumper.js": "^1.3.1",
        "jsdom": "^14.1.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.3.0",
        "vuepress": "^1.2.0",
        "webpack": "^4.41.1",
        "webpack-cli": "^3.3.9"
    },
    "dependencies": {
        "tone": "^13.8.25"
    }
